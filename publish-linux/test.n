start:
    call main
    out "welcome to hell"
end

.block main:
    inp r1
    out r1
    next

    db test 0
    inp test
    out test
    clear test
    next

    mov r1 50
    mov r2 10
    db n1 50
    dw n2 50
    dd n3 50
    dq n4 50
    ds n5 "Hello"
    ds n6 " "
    ds n7 "World!"

    add r1 10
    add r1 r1
    add r1 n1
    add r1 n2
    add r1 n3
    add r1 n4
    out "320 r1: "
    out r1
    next

    add n1 10
    add n1 n1
    add n1 r2
    out "130 n1: "
    out n1
    next
    
    add n2 10
    add n2 n2
    add n2 r2
    out "130 n2: "
    out n2
    next
 
    add n3 n3
    add n3 r2
    add n3 10
    out "120 n3: "
    out n3
    next


    add n4 n4
    add n4 r2
    add n4 10
    out "120 n4: "
    out n4
    next

    add n5 n6
    add n5 n7
    out n5
    next

    sub n4 n4
    sub n4 10
    sub n4 r2
    out "-20 n4: "
    out n4
    next

    mul n4 n4
    mul n4 10
    mul n4 r2
    out "40000 n4: "
    out n4
    next

    div r4 r4
    out n4
    next

    dq num1 1000
    dq num2 1000
    mov num1 num2
    out num1
    next

    ret

end

stop:
    clear registres
    clear n1
    clear n2
    clear n3
    clear n4
    clear n5
    clear n6
    clear n7
end