__start:
    go main

.p main:
    out, "Введите делимое число: "
    inp, r1
    out, "Введите делитель: "
    inp, r2

    div r1, r2

    cmp rnr, 0
    ife, go finish
    ifn, go nefinish

.p finish:
    out, "Остатка нету"
    hlt

.p nefinish:
    out, "Остаток есть: "
    out, rnr
    hlt

__stop:
    clear registres
